+   
+	};
+
+};
+
+&i2c2 {
+	tvp5150_1: tvp5150_1@5c {
+		compatible = "tvp5150";
+		reg = <0x5c>;
+		pinctrl-names = "default";
+		pinctrl-0 = <&pinctrl_ipu1>;
+		clock-names = "csi_mclk";
+		csi_id = <0>;
+	};
+	tvp5150_2: tvp5150_2@5d {
+		compatible = "tvp5150";
+		reg = <0x5d>;
+		pinctrl-names = "default";
+		pinctrl-0 = <&pinctrl_ipu2>;
+		clock-names = "csi_mclk";
+		csi_id = <1>;
+	};
+};
+
+&i2c3 {
+	tvp5150_3: tvp5150_3@5c {
+		compatible = "tvp5150";
+		reg = <0x5c>;
+	};
+	tvp5150_4: tvp5150_4@5d {
+		compatible = "tvp5150";
+		reg = <0x5d>;
+	};
+};
+
+&mxcfb1 {
+	status = "okay";
+};
+
+&mxcfb2 {
+	status = "okay";
+};
+
+&mxcfb3 {
