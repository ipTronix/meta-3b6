+/dts-v1/;
+
+#include "imx6q.dtsi"
+#include "imx6q-tera-he-dep.dtsi"
+#include "imx6q-tera-he-dep-iomux.dtsi"
+/ {
+	model = "3B6 iMX6 Tera HE Depopulated board";
+	compatible = "fsl,imx6q-tera-he", "fsl,imx6q";
+};
+
+&ldb {
+	ipu_id = <0>;
+	sec_ipu_id = <0>;
+	lvds-channel@0 {
+		crtc = "ipu1-di0";
+	};
+};
+
+&cpu0 {
+	arm-supply = <&sw3_reg>;
+	soc-supply = <&sw1_reg>;
+//TODO	pu-supply = <&pu_dummy>; /* use pu_dummy if VDDSOC share with VDDPU */
+};
+
+&i2c1 {
+	bq32000: rtc@68 {
+		compatible = "nxp,bq32000";
+		reg = <0x68>;
+	};
+
+	ltc3676: pmic@3c {
+		compatible = "lltc,ltc3676";
+		reg = <0x3c>;
+
+		regulators {
+			sw1_reg: sw1 {
+				regulator-min-microvolt = <779625>;
+				regulator-max-microvolt = <1512000>;
+				lltc,fb-voltage-divider = <178000 200000>;
+				regulator-ramp-delay = <7000>;
+				regulator-boot-on;
+				regulator-always-on;
+			};
+
+			sw2_reg: sw2 {
+				regulator-min-microvolt = <855937>;
+				regulator-max-microvolt = <1660000>;
+				lltc,fb-voltage-divider = <215000 200000>;
+				regulator-ramp-delay = <7000>;
+				regulator-boot-on;
+				regulator-always-on;
+			};
+
+			sw3_reg: sw3 {
+				regulator-min-microvolt = <779625>;
+				regulator-max-microvolt = <1512000>;
+				lltc,fb-voltage-divider = <178000 200000>;
+				regulator-ramp-delay = <7000>;
+				regulator-boot-on;
+				regulator-always-on;
+			};
+
+			sw4_reg: sw4 {
+				regulator-min-microvolt = <1887188>;
+				regulator-max-microvolt = <3660000>;
+				lltc,fb-voltage-divider = <715000 200000>;
+				regulator-ramp-delay = <7000>;
+				regulator-boot-on;
+				regulator-always-on;
+			};
+
+			ldo1_reg: ldo1 {
+				regulator-min-microvolt = <3300000>;
+				regulator-max-microvolt = <3300000>;
+				lltc,fb-voltage-divider = <0 0>;
+				regulator-boot-on;
+				regulator-always-on;
+			};
+
+			ldo2_reg: ldo2 {
+				regulator-min-microvolt = <2500000>;
+				regulator-max-microvolt = <2500000>;
+				lltc,fb-voltage-divider = <0 0>;
+				regulator-boot-on;
+				regulator-always-on;
+			};
+
+			ldo3_reg: ldo3 {
+				regulator-min-microvolt = <1800000>;
+				regulator-max-microvolt = <1800000>;
+				lltc,fb-voltage-divider = <0 0>;
+				regulator-boot-on;
+			};
+
+			ldo4_reg: ldo4 {
+				regulator-min-microvolt = <1800000>;
+				regulator-max-microvolt = <1800000>;
+				lltc,fb-voltage-divider = <0 0>;
+				regulator-boot-on;
+				regulator-always-on;
+			};
+		};
+	};
+
+	lsm330_acc: lsm330_acc@1d {
+		compatible = "st,lsm330_acc";
+		reg = <0x1d>;
+	};
+
+	lsm330_gyr: lsm330_gyr@6b {
+		compatible = "st,lsm330_gyr";
+		reg = <0x6b>;
+	};
+
+	codec: sgtl5000@0a {
+		compatible = "fsl,sgtl5000";
+		clocks = <&clks 201>;
+		reg = <0x0a>;
+		VDDA-supply = <&reg_1p8v>;
+		VDDIO-supply = <&reg_3p3v>;
+	};
+
+	isl29018: isl29018@44 {
+		compatible = "isil,isl29018";
+		reg = <0x44>;
+	};
+
+	pn547: pn547@29 {
+		compatible = "nxp,pn547";
+		reg = <0x24>;
+		clock-frequency = <400000>;
+		interrupt-gpios = <&gpio4 28 0>;
+		enable-gpios = <&gpio5 14 0>;
+	};
+
+};
+
+&i2c2 {
+	tvp5150_1: tvp5150_1@5d {
+		compatible = "tvp5150";
+		reg = <0x5d>;
+		pinctrl-names = "default";
+		pinctrl-0 = <&pinctrl_ipu1>;
+		clocks = <&clks 201>;
+		clock-names = "csi_mclk";
+		mclk = <27000000>;
+		mclk_source = <0>;
+		csi_id = <0>;
+	};
+
+	tvp5150_2: tvp5150_2@5c {
+		compatible = "tvp5150";
+		reg = <0x5c>;
+		pinctrl-names = "default";
+		pinctrl-0 = <&pinctrl_ipu2>;
+		clocks = <&clks 201>;
+		clock-names = "csi_mclk";
+		mclk = <27000000>;
+		mclk_source = <0>;
+		csi_id = <1>;
+	};
+};
+
+&i2c3 {
+	tvp5150_3: tvp5150_3@5d {
+		compatible = "tvp5150";
+		reg = <0x5d>;
+		clocks = <&clks 201>;
+		clock-names = "csi_mclk";
+		mclk = <27000000>;
+		mclk_source = <0>;
+		csi_id = <2>;
+	};
+
+	tvp5150_4: tvp5150_4@5c {
+		compatible = "tvp5150";
+		reg = <0x5c>;
+		clocks = <&clks 201>;
+		clock-names = "csi_mclk";
+		mclk = <27000000>;
+		mclk_source = <0>;
+		csi_id = <3>;
+	};
+};
+
+&ecspi2 {
+	status = "okay";
+
+	can4: can@4 {
+		compatible = "microchip,mcp2515";
+		reg = <0>;
+		spi-max-frequency = <5000000>;
+		clocks = <&can34_osc>;
+		interrupt-parent = <&gpio2>;
+		interrupts = <12 IRQ_TYPE_EDGE_FALLING>;
+		status = "okay";
+	};
+};
+
+&ecspi3 {
+	status = "okay";
+
+	can3: can@3 {
+		compatible = "microchip,mcp2515";
+		reg = <0>;
+		spi-max-frequency = <5000000>;
+		clocks = <&can34_osc>;
+		interrupt-parent = <&gpio2>;
+		interrupts = <14 IRQ_TYPE_EDGE_FALLING>;
+		status = "okay";
+	};
+};
+
+&mxcfb1 {
+	status = "okay";
+};
+
+&mxcfb2 {
+	status = "okay";
+};
+
+ &ssi1 {
+	fsl,mode = "i2s-slave";
+	status = "okay";
+};
